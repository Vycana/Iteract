{
    "env": {
        "browser": true,
        "es2021": true,
        "node": true
    },
    "extends": "eslint:recommended",
    "overrides": [
    ],
    "parserOptions": {
        "ecmaVersion": "latest",
        "sourceType": "module"
    },
    "rules": {
        "eqeqeq": "off",
        "quotes": ["error", "double"],
        "semi": ["error", "always"],
        "camelcase": "error",
        "class-methods-use-this": "error",
        "indent": ["error", "tab"]
    }
}
